{
  "name": "oanhnn/shopify-php-sdk",
  "type": "library",
  "description": "An unofficial Shopify PHP SDK",
  "keywords": [
    "shopify",
    "sdk",
    "php-sdk",
    "shopify-sdk"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oanhnn/shopify-php-sdk",
  "support": {
    "issues": "https://github.com/oanhnn/shopify-php-sdk/issues",
    "source": "https://github.com/oanhnn/shopify-php-sdk"
  },
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shopify\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "php-http/cache-plugin": "^1.4",
    "php-http/client-common": "^1.6",
    "php-http/client-implementation": "^1.0",
    "php-http/discovery": "^1.0",
    "php-http/httplug": "^1.1",
    "psr/cache": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "symfony/options-resolver": "^4.0",
    "symfony/serializer": "^4.0"
  },
  "require-dev": {
    "cache/array-adapter": "^0.4",
    "guzzlehttp/psr7": "^1.2",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/mock-client": "^1.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "phpunit": "php vendor/phpunit/phpunit/phpunit",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
